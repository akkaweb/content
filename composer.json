{
    "name": "aoliverio/content",
    "description": "Content is a CakePHP 3.x plugin for Content Management System backend in Bootstrap style.",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Content\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Content\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
